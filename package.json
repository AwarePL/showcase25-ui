{
  "name": "showcase25-ui",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.cypress.js",
    "format": "prettier --write **/*.{js,cypress.js}",
    "cypress:run:dev": "node cypressRunner.js --env dev",
    "cypress:run:staging": "node cypressRunner.js --env staging",
    "cypress:run:prod": "node cypressRunner.js --env prod",
    "cypress:open:dev": "node cypressRunner.js --env dev --open",
    "cypress:open:staging": "node cypressRunner.js --env staging --open",
    "cypress:open:prod": "node cypressRunner.js --env prod --open"
  },
  "repository": "https://github.com/AwarePL/showcase25-ui.git",
  "author": "AwarePL <lukasz@korchowiec.pl>",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "eslint": "^9.32.0",
    "eslint-plugin-cypress": "^5.1.0",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "cypress": "^14.5.3"
  }
}
